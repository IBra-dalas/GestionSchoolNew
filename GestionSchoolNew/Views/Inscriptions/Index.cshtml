@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EtatEleve)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item._Tranche.LibelleTranche)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item._Tranche.LibelleTranche)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item._AnneeScolaire.LibelleAnneeScolaire)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item._Classe.LibelleClasse)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item._ModePayement.LibelleModePayement)
        </td>

        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.IdInscription }) |
            @Html.ActionLink("Details", "Details", new { id = item.IdInscription }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.IdInscription })
        </td>
    </tr>
