@model IEnumerable<GestionSchoolNew.Models.Inscription>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EtatEleve)
        </th>

        <th>
            @Html.DisplayNameFor(model => model._Tranche.LibelleTranche)
        </th>
        <th>
            @Html.DisplayNameFor(model => model._AnneeScolaire.LibelleAnneeScolaire)
        </th>
